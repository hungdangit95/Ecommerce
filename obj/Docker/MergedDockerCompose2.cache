services:
  catalog.api:
    build:
      context: D:\Source\Ecommerce\Ecommerce
      dockerfile: Catalog.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Catalog.Api
      target: base
    container_name: Catalog.Api_1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: catalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/Catalog.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - D:\Source\Ecommerce\Ecommerce\Catalog.Api:/app:rw
    - C:\Users\HungDangHong\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\HungDangHong\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\HungDangHong\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\HungDangHong\.nuget\packages:/root/.nuget/packages:ro
    - D:\Source\Ecommerce\Ecommerce:/src:rw
version: '3.4'